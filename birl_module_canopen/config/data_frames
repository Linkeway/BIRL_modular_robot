  can0  RX - -  702   [1]  00				boot-up heartbeat
  can0  RX - -  082   [8]  30 81 00 00 00 40 00 00	Sync或者Heartbeat错误

  can0  TX - -  000   [2]  82 02			NMT模块控制 82：reset communication
  can0  RX - -  702   [1]  00				boot-up heartbeat

  can0  TX - -  602   [8]  2B 17 10 00 64 00 00 00	设置1017为100，heartbeat time
  can0  RX - -  582   [8]  60 17 10 00 00 00 00 00

  can0  TX - -  602   [8]  40 00 18 01 00 00 00 00	SDO查询1800sub1，PDO0 的COB-ID
  can0  RX - -  582   [8]  43 00 18 01 82 01 00 00	返回0x182

  can0  TX - -  602   [8]  23 00 18 01 82 01 00 80	设置1800sub1 0x182
  can0  RX - -  582   [8]  60 00 18 01 00 00 00 00

  can0  TX - -  602   [8]  2F 00 1A 00 00 00 00 00	设置1A00sub0 0 TxPDO0 映射到PDO0的对象数 为0
  can0  RX - -  582   [8]  60 00 1A 00 00 00 00 00

  can0  TX - -  602   [8]  23 00 1A 01 10 00 41 60	设置1A00sub1为0x60410010 映射对象6041(statusword), 16位组成
  can0  RX - -  582   [8]  60 00 1A 01 00 00 00 00

  can0  TX - -  602   [8]  40 41 60 00 00 00 00 00	读取6041(statusword)
  can0  RX - -  582   [8]  4B 41 60 00 70 07 41 60	返回0x6041 0770

  can0  TX - -  602   [8]  23 00 1A 02 08 00 61 60	设置1A00sub2 0x60610008 映射对象6061 子索引0 8位组成
  can0  RX - -  582   [8]  60 00 1A 02 00 00 00 00

  can0  TX - -  602   [8]  40 61 60 00 00 00 00 00	读取6061 current mode of operation
  can0  RX - -  582   [8]  4F 61 60 00 00 00 61 60	返回 0

  can0  TX - -  602   [8]  23 00 1A 03 20 00 41 20	设置1A00sub3 0x20410020 映射对象2041 子索引0 32位组成
  can0  RX - -  582   [8]  80 00 1A 03 41 00 04 06	报错：对象不能映射到PDO

  can0  TX - -  602   [8]  2B 17 10 00 00 00 00 00	设置1017 0， disable heartbeat
  can0  RX - -  582   [8]  60 17 10 00 00 00 00 00

  can0  TX - -  000   [2]  02 02			NMT模块控制 02：stop remote node

